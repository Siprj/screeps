{
  "extends" : [
    "tslint:recommended"
  ],
  "rules": {
    "one-line": false,
    "forin": false,
    "interface-name": [false],
    "no-console": [false],
    "no-namespace": false,
    "no-string-literal": false,
    "trailing-comma":      [
        true,
        { "multiline": "never",
          "singleline": "never"
        }
      ],
    "typedef-whitespace": [false],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ]
  }
}
